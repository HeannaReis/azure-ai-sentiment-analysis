        """Verifica e configura as listas do quadro Trello."""
        lists = self.board.open_lists()
        for lst in lists:
            if lst.name == "Backlog":
                self.backlog_list = lst
            elif lst.name == "Em Andamento":
                self.in_progress_list = lst
            elif lst.name == "Em Teste":
                self.testing_list = lst
            elif lst.name == "Concluído":
                self.done_list = lst
        if not self.backlog_list:
            self.backlog_list = self.board.add_list("Backlog")
        if not self.in_progress_list:
            self.in_progress_list = self.board.add_list("Em Andamento")
        if not self.testing_list:
            self.testing_list = self.board.add_list("Em Teste")
        if not self.done_list:
            self.done_list = self.board.add_list("Concluído")
    def adicionar_tarefa_backlog(self, nome_tarefa, descricao_tarefa, responsavel=None):
        """Adiciona uma nova tarefa ao Backlog."""
        if self.backlog_list:
            card = self.backlog_list.add_card(nome_tarefa, desc=descricao_tarefa)
            if responsavel:
                card.assign(responsavel)
        else:
            print("Lista 'Backlog' não encontrada.")
    def mover_tarefa(self, nome_tarefa, lista_destino):
        """Move uma tarefa existente para uma lista específica."""
        card = self._encontrar_tarefa(nome_tarefa)
        if card:
            if lista_destino == "Em Andamento":
                card.change_list(self.in_progress_list.id)
            elif lista_destino == "Em Teste":
                card.change_list(self.testing_list.id)
            elif lista_destino == "Concluído":
                card.change_list(self.done_list.id)
            elif lista_destino == "Backlog":
                card.change_list(self.backlog_list.id)
            else:
                print("Lista de destino inválida.")
        else:
            print(f"Tarefa '{nome_tarefa}' não encontrada.")
    def _encontrar_tarefa(self, nome_tarefa):
        """Localiza uma tarefa com base no nome em todas as listas do quadro."""
        all_cards = self.board.get_cards()
        for card in all_cards:
            if card.name == nome_tarefa:
                return card
        return None
    def definir_responsavel(self, nome_tarefa, responsavel):
        """Define ou altera o responsável por uma tarefa."""
        card = self._encontrar_tarefa(nome_tarefa)
        if card:
            card.assign(responsavel)
        else:
            print(f"Tarefa '{nome_tarefa}' não encontrada.")
    def listar_tarefas(self, lista):
        """Lista todas as tarefas em uma determinada lista do quadro."""
        if lista == "Backlog":
            cards = self.backlog_list.list_cards()
        elif lista == "Em Andamento":
            cards = self.in_progress_list.list_cards()
        elif lista == "Em Teste":
            cards = self.testing_list.list_cards()
        elif lista == "Concluído":
            cards = self.done_list.list_cards()
        else:
