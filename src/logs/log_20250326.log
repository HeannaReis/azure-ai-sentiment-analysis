            except Exception as e:
                response = f"❌ Erro ao gerar resposta: {str(e)}"
                st.error(response)

    # Adiciona resposta ao histórico
    st.session_state.messages.append({"role": "assistant", "content": response})

    # Remove imagem após uso
    if img_path and os.path.exists(img_path):
        os.remove(img_path)

    st.session_state.processing = False  # Libera para a próxima mensagem
    process_next_message()  # Chama a função novamente para processar a próxima mensagem da fila

# Adiciona nova mensagem à fila sem bloquear a interface
if user_input or uploaded_image:
    st.session_state.message_queue.append((user_input, uploaded_image))
    process_next_message()  # Inicia o processamento da fila
